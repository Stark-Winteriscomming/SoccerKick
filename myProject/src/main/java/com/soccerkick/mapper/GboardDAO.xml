<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.soccerkick.dao.GboardDAO">
	<insert id="insertApply">
	insert into apply(a_applicant, a_apy_position) values(#{a_applicant},#{a_apy_position})
	</insert>
	
	<select id="execSelect"  resultType="com.soccerkick.vo.TeamVO">
		select * from team2
	</select>
	
	<select id="execContent" resultType="com.soccerkick.vo.TeamVO">
		select team_id, team_name, team_logo_file_name, team_formation, client_id from team2 where team_id=#{team_id}	
	</select>
	
</mapper>     