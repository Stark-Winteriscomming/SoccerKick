<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.soccerkick.dao.JoinDAO">
	<insert id="execInsert">
		insert into client(client_id,client_name,client_phone,pw, fname) values(#{client_id},#{client_name},#{client_phone},#{pw}, #{fname})
	</insert>
</mapper>    